    @cache {
        mode = 'cached'
        entryIdentifier {
            1 = ${props.path}
        }
        entryTags {
            1 = ${props.path}
        }
    }

    @class = 'CRON\\NeosHashedAssets\\FusionObjects\\HashedResourceUriImplementation'

    @exceptionHandler = 'Neos\\Fusion\\Core\\ExceptionHandlers\\AbsorbingHandler'
}
